# n8n master .env.example (Docker Compose + workflow env vars)
# Copy to `.env` next to `01_DOCKER/docker-compose.yml`.

### Core
POSTGRES_USER=lsn
POSTGRES_PASSWORD=change_me
POSTGRES_DB=lsn

N8N_PORT=5678
# If deploying behind a domain + TLS, set these:
N8N_HOST=localhost
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# REQUIRED for n8n encryption at rest (32+ chars):
N8N_ENCRYPTION_KEY=

### Storage (MinIO)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_BUCKET=lsn-assets
PUBLIC_ASSET_BASE_URL=http://localhost:9000/lsn-assets

### LSN pipeline
LSN_OUTPUT_DIR=/data/LSN_OUTPUTS
COMFYUI_BASE_URL=http://host.docker.internal:8188

# API keys
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
HEYGEN_API_KEY=
PIAPI_KLING_API_KEY=
SEGMIND_API_KEY=
GOOGLE_API_KEY=
GOOGLE_PROJECT_ID=
GOOGLE_REGION=us-central1

# Workflow IDs (filled AFTER importing into n8n)
WF_LISTING_ID=
WF_VOICE_ID=
WF_TALKING_ID=
WF_KLING_ID=
WF_CINEMATIC_ID=
WF_UPLOAD_ID=
WF_VOICE_UPLOAD_ID=
WF_RENDER_UPLOAD_ID=

### AI Business OS kernel
AIOS_BASE_URL=http://localhost:5678
# Sub-workflow IDs (filled AFTER importing)
AIOS_WF_POLICY_GATE_ID=
AIOS_WF_APPROVAL_REQUEST_ID=
AIOS_WF_CONTENT_FACTORY_ID=
AIOS_WF_FINANCE_GATE_ID=
AIOS_WF_KILL_SWITCH_ID=

# Optional Slack
AIOS_SLACK_APPROVAL_CHANNEL=#approvals
AIOS_SLACK_INCIDENT_CHANNEL=#incidents

